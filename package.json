{
  "name": "umb-vue",
  "version": "0.1.2",
  "packageManager": "pnpm@10.2.0",
  "description": "Vue.js custom elements integration for Umbraco backoffice",
  "type": "module",
  "keywords": [
    "unplugin",
    "vue",
    "umbraco",
    "backoffice",
    "custom-elements",
    "web-components",
    "vite",
    "rollup",
    "rolldown",
    "esbuild"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JoJk0/umb-vue#readme",
  "bugs": {
    "url": "https://github.com/JoJk0/umb-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoJk0/umb-vue.git"
  },
  "author": "jojk0 <jacob@jojko.tech>",
  "funding": "https://github.com/sponsors/jojk0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./vite": "./dist/vite.js",
    "./rollup": "./dist/rollup.js",
    "./rolldown": "./dist/rolldown.js",
    "./esbuild": "./dist/esbuild.js",
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@umbraco-cms/backoffice": "^14.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.4",
    "@vue/shared": "^3.5.13",
    "@vueuse/rxjs": "^12.5.0",
    "unplugin": "^2.1.2"
  },
  "devDependencies": {
    "@babel/types": "^7.26.7",
    "@sxzz/eslint-config": "^5.0.1",
    "@sxzz/prettier-config": "^2.1.1",
    "@sxzz/test-utils": "^0.5.1",
    "@types/node": "^22.10.1",
    "@umbraco-cms/backoffice": "15.2.0-rc",
    "@vue-macros/common": "^1.16.1",
    "ast-walker-scope": "^0.6.2",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "oxc-transform": "^0.48.2",
    "prettier": "^3.4.2",
    "tsdown": "^0.5.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@sxzz/prettier-config"
}
